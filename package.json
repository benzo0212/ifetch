{
  "name": "ifetch-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "npm run -w client install && npm run -w server install",
    "dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=server\"",
    "dev:client": "npm --workspace=client run dev",
    "dev:server": "npm --workspace=server run dev",
    "build": "npm --workspace=client run build",
    "start": "node server/index.js",
    "preview": "npm --workspace=client run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.22.1"
  }
}
